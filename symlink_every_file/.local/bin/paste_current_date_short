#!/bin/bash

# https://github.com/gotbletu/shownotes/blob/master/snippy.sh

# Put the selected string (without the trailing newline) into the paste buffer.
#xdotool type $(/bin/date '+%F (%A)')
# Paste into the current application.
#xdotool key ctrl+v    #gui paste
#xdotool key ctrl+shift+v #cli
STRING=$(/bin/date '+%F')
xdotool type --delay 0 "$STRING"
